<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_empty_model_form" model="ir.ui.view">
        <field name="name">empty.model.form</field>
        <field name="model">empty.model</field>
        <field name="arch" type="xml">
                <form>
                    <field name="name"/>
                    <field name="age"/>
                </form>
        </field>
    </record>

    <record id="action_empty_module" model="ir.actions.act_window">
        <field name="name">Empty Records</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">empty.model</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_empty_module_action"
              name="Empty Records"
              parent="menu_empty_operations"
              action="action_empty_module"
              sequence="10"/>
</odoo>